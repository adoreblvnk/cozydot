# if you see this file being updated, run:
# git update-index --assume-unchanged dotfiles/bash/.gitconfig
[user]
	name =
    email =
    signingkey =
[credential]
    credentialStore = gpg
    helper = /usr/local/bin/git-credential-manager
[credential "https://dev.azure.com"]
    useHttpPath = true
[commit]
    gpgsign = true
[tag]
    gpgsign = true
[alias]
    ll = log --pretty=format:'%C(yellow)%h%C(auto)%d %Creset%s %C(magenta)%cr' -n 10
    lm = log --graph --pretty=format:'%C(yellow)%h %C(magenta)%ch%C(auto)%d %C(reset)%s %C(blue)<%an>'
